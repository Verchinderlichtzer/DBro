<Router AppAssembly="typeof(Program).Assembly">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(MainLayout)">
            <NotAuthorized>
                <DBro.Web.Components.Partial.HttpUnauthorized />
            </NotAuthorized>
        </AuthorizeRouteView>
    </Found>
    <NotFound>
        <PageTitle>Halaman Tidak Ditemukan</PageTitle>
        <LayoutView Layout="@typeof(NoLayout)">
            <ChildContent>
                <DBro.Web.Components.Partial.HttpNotFound />
            </ChildContent>
        </LayoutView>
    </NotFound>
</Router>